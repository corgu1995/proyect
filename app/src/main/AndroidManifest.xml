<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.santiagoromeroinv">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/pencil"
        android:label="@string/app_name"
        android:supportsRtl="true"

        android:theme="@style/AppTheme">
        <activity android:name="com.example.santiagoromeroinv.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.santiagoromeroinv.drawing.DrawingActivity"/>
        <activity android:name="com.example.santiagoromeroinv.login.RegisterActivity"/>
        <activity android:name="com.example.santiagoromeroinv.main.MainActivity"/>
    </application>

</manifest>